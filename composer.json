{
    "name": "yudina/laravel-sms",
    "description": "Sms provider",
    "keywords": ["laravel", "sms"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Daria Yudina",
            "email": "dasha_2195@mail.ru"
        }
    ],
    "require": {
        "php": ">=7.1|>=8.0",
        "illuminate/support": "^5.8|^6.0|^7.0|^8.0",
        "illuminate/notifications": "^5.8|^6.0|^7.0|^8.0",
        "guzzlehttp/guzzle": "^6.0|^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Yudina\\LaravelSms\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Yudina\\LaravelSms\\SmsSenderServiceProvider"
            ],
            "aliases": {
                "SmsSender": "Yudina\\LaravelSms\\Facades\\SmsSender"
            }
        }
    }
}
